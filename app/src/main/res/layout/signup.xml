<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent">

  <androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
      android:id="@+id/imageView7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="10dp"
      android:layout_marginBottom="30dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView8"
      app:srcCompat="@drawable/signup" />

    <ImageView
      android:id="@+id/imageView8"
      android:layout_width="90dp"
      android:layout_height="90dp"
      android:layout_marginTop="50dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/login_logo" />

  </androidx.constraintlayout.widget.ConstraintLayout>

  <ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    app:layout_constraintEnd_toEndOf="@+id/constraintLayout"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/constraintLayout">

    <androidx.constraintlayout.widget.ConstraintLayout
      android:layout_width="match_parent"
      android:layout_height="match_parent">

      <EditText
        android:id="@+id/et_signpw"
        android:layout_width="279dp"
        android:layout_height="35dp"
        android:background="@drawable/radius"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="229dp" />

      <EditText
        android:id="@+id/et_phone"
        android:layout_width="279dp"
        android:layout_height="35dp"
        android:background="@drawable/radius"
        app:layout_constraintEnd_toEndOf="@+id/et_signname"
        app:layout_constraintStart_toStartOf="@+id/et_signname"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="407dp" />

      <TextView
        android:id="@+id/tv_signaddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:text="주소"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_address"
        app:layout_constraintStart_toStartOf="@+id/et_address"
        tools:ignore="MissingConstraints" />

      <TextView
        android:id="@+id/tv_signid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:text="ID"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_signpw"
        app:layout_constraintStart_toStartOf="@+id/et_signpw"
        tools:ignore="MissingConstraints" />

      <EditText
        android:id="@+id/et_address"
        android:layout_width="279dp"
        android:layout_height="35dp"
        android:background="@drawable/radius"
        app:layout_constraintEnd_toEndOf="@+id/et_phone"
        app:layout_constraintStart_toStartOf="@+id/et_phone"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="463dp" />

      <EditText
        android:id="@+id/et_univ"
        android:layout_width="279dp"
        android:layout_height="35dp"
        android:background="@drawable/radius"
        app:layout_constraintEnd_toEndOf="@+id/et_address"
        app:layout_constraintStart_toStartOf="@+id/et_address"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="517dp" />

      <TextView
        android:id="@+id/tv_signschool"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:text="학교"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_univ"
        app:layout_constraintStart_toStartOf="@+id/et_univ"
        tools:ignore="MissingConstraints" />

      <TextView
        android:id="@+id/tv_signphone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:text="전화번호"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_phone"
        app:layout_constraintStart_toStartOf="@+id/et_phone"
        tools:ignore="MissingConstraints" />

      <EditText
        android:id="@+id/et_signid"
        android:layout_width="279dp"
        android:layout_height="35dp"
        android:background="@drawable/radius"
        app:layout_constraintEnd_toEndOf="@+id/et_signpw"
        app:layout_constraintStart_toStartOf="@+id/et_signpw"
        app:layout_constraintTop_toBottomOf="@+id/et_signpw"
        tools:ignore="MissingConstraints" />

      <TextView
        android:id="@+id/tv_signname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:text="성명"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_signname"
        app:layout_constraintStart_toStartOf="@+id/et_signname"
        tools:ignore="MissingConstraints" />

      <EditText
        android:id="@+id/et_nickname"
        android:layout_width="279dp"
        android:layout_height="35dp"
        android:background="@drawable/radius"
        app:layout_constraintEnd_toEndOf="@+id/et_univ"
        app:layout_constraintStart_toStartOf="@+id/et_univ"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="579dp" />

      <EditText
        android:id="@+id/et_signname"
        android:layout_width="279dp"
        android:layout_height="35dp"
        android:background="@drawable/radius"
        app:layout_constraintTop_toBottomOf="@+id/et_signid"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="66dp" />

      <TextView
        android:id="@+id/tv_signnickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:text="닉네임"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_nickname"
        app:layout_constraintStart_toStartOf="@+id/et_nickname"
        tools:ignore="MissingConstraints" />

      <TextView
        android:id="@+id/tv_signpw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:text="PW"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_signid"
        app:layout_constraintStart_toStartOf="@+id/et_signid"
        tools:ignore="MissingConstraints" />


    </androidx.constraintlayout.widget.ConstraintLayout>


  </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>